alias cp="cp -iv"
alias mv="mv -iv"
alias mkdir="mkdir -pv"
alias ll="ls -al"
alias sources="source ~/.zshrc"
del() { rm -rf "$*" }
alias c="clear"
alias editzsh="vim ~/.zshrc"
alias editgit="vim ~/.gitconfig"
alias sysupdate="sudo apt update; sudo apt upgrade -y; sudo apt autoremove -y; sudo apt autoclean -y"

# Switch repos
alias h="cd ~/"
alias w="cd ~/project"

# GIT Aliases
alias st="git status"
alias gp="git push"
alias push="git push --no-verify"
alias done="git push origin HEAD"
alias log="git log --pretty=format:'%C(magenta)%h%Creset -%C(red)%d%Creset %s %C(dim green)(%cr) [%an]'"
alias gclean="git clean -xdf"
alias pop="git stash pop"
alias stash="git stash -u"
alias apply="git stash apply"
alias prune="git prune"
alias pull="git pull"
alias fetch="git fetch --prune"
alias fp="fetch && pull"
alias resolve="git add -A && git commit --no-edit"
alias track="git push -u --no-verify"

cm() { git add -A; git commit -m "$*" }
cmt() { git add -A; git commit -m "$*"; git push }

# delete merged branch
alias dmb="git branch --merged | grep -v "*" | xargs git branch -D"

# Remove all unused images not just dangling ones
# Do not prompt for confirmation
# Prune volumes
dclean(){ docker system prune --volumes --all --force }

# spaceship configs
SPACESHIP_PACKAGE_SHOW="false"
SPACESHIP_NODE_SHOW="false"
SPACESHIP_DIR_TRUNC_REPO="false"
SPACESHIP_DIR_TRUNC="0"

SPACESHIP_GIT_STATUS_PREFIX=" ["
SPACESHIP_GIT_STATUS_SUFFIX=" ]"
SPACESHIP_GIT_STATUS_UNTRACKED=" UNTRACKED"
SPACESHIP_GIT_STATUS_ADDED=" ADDED"
SPACESHIP_GIT_STATUS_MODIFIED=" MODIFIED"
SPACESHIP_GIT_STATUS_RENAMED=" RENAMED"
SPACESHIP_GIT_STATUS_DELETED=" DELETED"
SPACESHIP_GIT_STATUS_STASHED=" STASHED"
SPACESHIP_GIT_STATUS_UNMERGED=" UNMERGED"
SPACESHIP_GIT_STATUS_AHEAD=" PUSH"
SPACESHIP_GIT_STATUS_BEHIND=" PULL"
SPACESHIP_GIT_STATUS_DIVERGED=" DIVERGED"
